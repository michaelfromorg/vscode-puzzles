{
  "name": "vscode-puzzle",
  "displayName": "Puzzles",
  "version": "1.1.1",
  "publisher": "michaelfromyeg",
  "description": "A VSCode extension to work on coding interview problems in your local text editor.",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "icon": "images/vscode-puzzle-logo-small.png",
  "galleryBanner": {
    "color": "#006400",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelfromyeg/vscode-puzzles.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extsn.getReddit",
        "title": "Puzzles: r/dailyprogrammer"
      },
      {
        "command": "extsn.getProjectEuler",
        "title": "Puzzles: Project Euler"
      },
      {
        "command": "extsn.getCodingBat",
        "title": "Puzzles: Coding Bat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint --fix src --ext ts",
    "dev": "npm watch",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "html-entities": "^1.3.1",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/mustache": "^4.0.1",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
