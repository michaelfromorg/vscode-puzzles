  },
  "homepage": "https://github.com/michaelfromyeg/vscode-puzzles#readme",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/rate-limit": "^10.2.1",
    "axios": "^1.7.8",
    "cheerio": "^1.0.0",
    "fastify": "^5.1.0",
    "he": "^1.2.0",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {
    "@types/he": "^1.2.3",
